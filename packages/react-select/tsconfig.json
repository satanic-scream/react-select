{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "jsx": "react",
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "alwaysStrict":true,
    "allowUnreachableCode":false,
    "noImplicitAny":true,
    "noImplicitThis":true,
    "noUnusedLocals":true,
    "noUnusedParameters":true,
    "noImplicitReturns":true,
    "noFallthroughCasesInSwitch":true,
  }
}
